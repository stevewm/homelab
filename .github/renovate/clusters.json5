{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  packageRules: [
    {
      description: "main cluster",
      matchFileNames: ["**/kubernetes/main/**"],
      additionalBranchPrefix: "main-",
    },
    {
      description: "NAS",
      matchFileNames: ["**/nas/**"],
      additionalBranchPrefix: "nas-",
    },
  ],
}
